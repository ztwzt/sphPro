rademark = `${trademark.tmId}:${trademark.tmName}`;\n      this.getData();\n    },\n    //自定义事件--收集attrInfo\n    attrInfo(attr, attrValue) {\n      let props = `${attr.attrId}:${attrValue}:${attr.attrName}`;\n      this.searchParams.props.push(props);\n      //需要去重\n      if (this.searchParams.params.indexOf(props) == -1) this.this.getData();\n    }\n  },\n  watch: {\n    //监听路由信息\n    $route(newValue, oldValue) {\n      //再次整理参数\n      Object.assign(this.searchParams, this.$route.query, this.$route.params);\n      this.getData();\n      //请求完毕，清理相应参数值\n      this.searchParams.categoryId1 = '';\n      this.searchParams.categoryId2 = '';\n      this.searchParams.categoryId3 = '';\n    }\n  }\n};","map":{"version":3,"names":["mapState","mapGetters","SearchSelector","name","components","data","searchParams","categoryId1","categoryId2","categoryId3","categoryname","keyword","order","trademark","pageNo","pageSize","props","beforeMount","Object","assign","$route","query","params","mounted","getData","computed","methods","$store","dispatch","removeCategoryName","undefined","$router","push","removeKeyword","$bus","$emit","removeTrademark","tradeMarkInfo","tmId","tmName","attrInfo","attr","attrValue","attrId","attrName","indexOf","this","watch","newValue","oldValue"],"sources":["src/pages/Search/index.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <TypeNav />\r\n    <div class=\"main\">\r\n      <div class=\"py-container\">\r\n        <!--bread 面包屑结构 -->\r\n        <div class=\"bread\">\r\n          <ul class=\"fl sui-breadcrumb\">\r\n            <li>\r\n              <a href=\"#\">全部结果</a>\r\n            </li>\r\n          </ul>\r\n          <ul class=\"fl sui-tag\">\r\n            <li class=\"with-x\" v-if=\"searchParams.categoryname\">{{ searchParams.categoryname  }}<i @click=\"removeCategoryName\">×</i></li>\r\n            <li class=\"with-x\" v-if=\"searchParams.keyword\">{{ searchParams.keyword  }}<i @click=\"removeKeyword\">×</i></li>\r\n            <li class=\