{"ast":null,"code":"import { mapGetters } from 'vuex';\nexport default {\n  name: 'ShopCart',\n  methods: {\n    getData() {\n      //暂时得不到数据，服务器不知道你的身份--uuid 游客身份\n      this.$store.dispatch('getShopCarList');\n    },\n    //修改数量 axios=>Num 最终量 type 区分 cart 目标  \n    async handler(type, disNum, cart) {\n      switch (type) {\n        case 'add':\n          disNum = 1;\n          break;\n        case 'minus':\n          disNum = cart.skuNum > 1 ? -1 : 0;\n          break;\n        case 'change':\n          if (isNaN(disNum) || disNum < 1) {\n            disNum = 0;\n          } else {\n            disNum = parseInt(disNum) - cart.skuNum;\n          }\n          break;\n      }\n      try {\n        await this.$store.dispatch('addOrUpdateShopCar', {\n          skuId: cart.skuId,\n          skuNum: disNum\n        });\n        this.getData();\n      } catch (error) {}\n    },\n    //删除\n    deleteCartById(cart) {}\n  },\n  mounted() {\n    this.getData();\n  },\n  computed: {\n    ...mapGetters(['carList']),\n    carInfoList() {\n      return this.carList.cartInfoList || [];\n    },\n    //计算总价\n    totalPrice() {\n      let sum = 0;\n      this.carInfoList.forEach(item => {\n        sum += item.skuNum * item.skuPrice;\n      });\n      return sum;\n    },\n    //allCheck\n    isCheckAll() {\n      return this.carInfoList.every(item => this.isChecked == 1);\n    }\n  }\n};","map":{"version":3,"names":["mapGetters","name","methods","getData","$store","dispatch","handler","type","disNum","cart","skuNum","isNaN","parseInt","skuId","error","deleteCartById","mounted","computed","carInfoList","carList","cartInfoList","totalPrice","sum","forEach","item","skuPrice","isCheckAll","every","isChecked"],"sources":["src/pages/ShopCart/index.vue"],"sourcesContent":["<template>\r\n  <div class=\"cart\">\r\n    <h4>全部商品</h4>\r\n    <div class=\"cart-main\">\r\n      <div class=\"cart-th\">\r\n        <div class=\"cart-th1\">全部</div>\r\n        <div class=\"cart-th2\">商品</div>\r\n        <div class=\"cart-th3\">单价（元）</div>\r\n        <div class=\"cart-th4\">数量</div>\r\n        <div class=\"cart-th5\">小计（元）</div>\r\n        <div class=\"cart-th6\">操作</div>\r\n      </div>\r\n      <div class=\"cart-body\">\r\n        <ul class=\"cart-list\" v-for=\"(cart,index) in carInfoList\" :key=\"cart.id\">\r\n          <li class=\"cart-list-con1\">\r\n            <input type=\"checkbox\" name=\"chk_list\" :checked=\"cart.isChecked==1\">\r\n          </li>\r\n          <li class=\"cart-list-con2\">\r\n            <img :src=\"cart.imgUrl\">\r\n            <div class=\"item-msg\">{{ cart.skuName }}</div>\r\n          </li>\r\n          <li class=\"cart-list-con4\">\r\n            <span class=\"price\">{{ cart.skuPrice}}</span>\r\n          </li>\r\n          <li class=\"cart-list-con5\">\r\n            <a href=\"javascript:void(0)\" class=\"mins\" \r\n            @click=\"handler('minus',-1,cart)\">-</a>\r\n            <input autocomplete=\"off\" type=\"text\" minnum=\"1\" class=\"itxt\" :value=\"cart.skuNum\" \r\n            @click=\"handler('change',$event.target.value*1,cart)\">\r\n            <a href=\"javascript:void(0)\" class=\"plus\"\r\n            @click=\"handler('add',1,cart)\">+</a>\r\n          </li>\r\n          <li class=\"cart-list-con6\">\r\n            <span class=\"sum\">{{ cart.skuNum *cart.skuPrice }}</span>\r\n          </li>\r\n          <li class=\"cart-list-con7\">\r\n            <a href=\"#none\" class=\"sindelet\" @click=\"deleteCartById(cart)\">删除</a>\r\n            <br>\r\n            <a href=\"#none\">移到收藏</a>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n    <div class=\"cart-tool\">\r\n      <div class=\"select-all\">\r\n        <input class=\"chooseAll\" type=\"checkbox\" :checked=\"isCheckAll\">\r\n        <span>全选</span>\r\n      </div>\r\n      <div class=\"option\">\r\n        <a href=\"#none\">删除选中的商品</a>\r\n        <a href=\"#none\">移到我的关注</a>\r\n        <a href=\"#none\">清除下柜商品</a>\r\n      </div>\r\n      <div class=\"money-box\">\r\n        <div class=\"chosed\">已选择\r\n          <span>0</span>件商品</div>\r\n        <div class=\"sumprice\">\r\n          <em>总价（不含运费） ：</em>\r\n          <i class=\"summoney\"> {{ totalPrice }}</i>\r\n        </div>\r\n        <div class=\"sumbtn\">\r\n          <a class=\"sum-btn\" href=\"###\" target=\"_blank\">结算</a>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex';\r\n  export default {\r\n    name: 'ShopCart',\r\n    methods:{\r\n      getData(){\r\n        //暂时得不到数据，服务器不知道你的身份--uuid 游客身份\r\n         this.$store.dispatch('getShopCarList')\r\n      },\r\n      //修改数量 axios=>Num 最终量 type 区分 cart 目标  \r\n     async handler(type,disNum,cart){\r\n        switch(type){\r\n          case 'add':\r\n              disNum=1\r\n              break\r\n          case 'minus':\r\n              disNum=cart.skuNum > 1 ? -1 : 0\r\n              break\r\n          case 'change':\r\n              if(isNaN(disNum) || disNum<1)\r\n              {\r\n                disNum=0\r\n              }else{\r\n                disNum=parseInt(disNum)-cart.skuNum\r\n              }\r\n              break\r\n        }\r\n        try{ \r\n         await this.$store.dispatch('addOrUpdateShopCar',{skuId:cart.skuId,skuNum:disNum})\r\n         this.getData()\r\n        }catch(error){\r\n\r\n        }\r\n      },\r\n      //删除\r\n      deleteCartById(cart){\r\n        \r\n      }\r\n    },\r\n    mounted(){\r\n     this.getData()\r\n    },\r\n    computed:{\r\n      ...mapGetters(['carList']),\r\n      carInfoList(){\r\n        return this.carList.cartInfoList ||[]\r\n      },\r\n      //计算总价\r\n      totalPrice(){\r\n        let sum=0\r\n        this.carInfoList.forEach(item => {\r\n            sum+=item.skuNum*item.skuPrice\r\n        });\r\n        return sum\r\n      },\r\n      //allCheck\r\n      isCheckAll(){\r\n          return this.carInfoList.every(item=>this.isChecked==1)\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n  .cart {\r\n    width: 1200px;\r\n    margin: 0 auto;\r\n\r\n    h4 {\r\n      margin: 9px 0;\r\n      font-size: 14px;\r\n      line-height: 21px;\r\n    }\r\n\r\n    .cart-main {\r\n      .cart-th {\r\n        background: #f5f5f5;\r\n        border: 1px solid #ddd;\r\n        padding: 10px;\r\n        overflow: hidden;\r\n\r\n        &>div {\r\n          float: left;\r\n        }\r\n\r\n        .cart-th1 {\r\n          width: 25%;\r\n\r\n          input {\r\n            vertical-align: middle;\r\n          }\r\n\r\n          span {\r\n            vertical-align: middle;\r\n          }\r\n        }\r\n\r\n        .cart-th2 {\r\n          width: 25%;\r\n        }\r\n\r\n        .cart-th3,\r\n        .cart-th4,\r\n        .cart-th5,\r\n        .cart-th6 {\r\n          width: 12.5%;\r\n\r\n        }\r\n      }\r\n\r\n      .cart-body {\r\n        margin: 15px 0;\r\n        border: 1px solid #ddd;\r\n\r\n        .cart-list {\r\n          padding: 10px;\r\n          border-bottom: 1px solid #ddd;\r\n          overflow: hidden;\r\n\r\n          &>li {\r\n            float: left;\r\n          }\r\n\r\n          .cart-list-con1 {\r\n            width: 15%;\r\n          }\r\n\r\n          .cart-list-con2 {\r\n            width: 35%;\r\n\r\n            img {\r\n              width: 82px;\r\n              height: 82px;\r\n              float: left;\r\n            }\r\n\r\n            .item-msg {\r\n              float: left;\r\n              width: 150px;\r\n              margin: 0 10px;\r\n              line-height: 18px;\r\n            }\r\n          }\r\n\r\n          .cart-list-con4 {\r\n            width: 10%;\r\n\r\n          }\r\n\r\n          .cart-list-con5 {\r\n            width: 17%;\r\n\r\n            .mins {\r\n              border: 1px solid #ddd;\r\n              border-right: 0;\r\n              float: left;\r\n              color: #666;\r\n              width: 6px;\r\n              text-align: center;\r\n              padding: 8px;\r\n            }\r\n\r\n            input {\r\n              border: 1px solid #ddd;\r\n              width: 40px;\r\n              height: 33px;\r\n              float: left;\r\n              text-align: center;\r\n              font-size: 14px;\r\n            }\r\n\r\n            .plus {\r\n              border: 1px solid #ddd;\r\n              border-left: 0;\r\n              float: left;\r\n              color: #666;\r\n              width: 6px;\r\n              text-align: center;\r\n              padding: 8px;\r\n            }\r\n          }\r\n\r\n          .cart-list-con6 {\r\n            width: 10%;\r\n\r\n            .sum {\r\n              font-size: 16px;\r\n            }\r\n          }\r\n\r\n          .cart-list-con7 {\r\n            width: 13%;\r\n\r\n            a {\r\n              color: #666;\r\n            }\r\n          }\r\n        }\r\n      }\r\n    }\r\n\r\n    .cart-tool {\r\n      overflow: hidden;\r\n      border: 1px solid #ddd;\r\n\r\n      .select-all {\r\n        padding: 10px;\r\n        overflow: hidden;\r\n        float: left;\r\n\r\n        span {\r\n          vertical-align: middle;\r\n        }\r\n\r\n        input {\r\n          vertical-align: middle;\r\n        }\r\n      }\r\n\r\n      .option {\r\n        padding: 10px;\r\n        overflow: hidden;\r\n        float: left;\r\n\r\n        a {\r\n          float: left;\r\n          padding: 0 10px;\r\n          color: #666;\r\n        }\r\n      }\r\n\r\n      .money-box {\r\n        float: right;\r\n\r\n        .chosed {\r\n          line-height: 26px;\r\n          float: left;\r\n          padding: 0 10px;\r\n        }\r\n\r\n        .sumprice {\r\n          width: 200px;\r\n          line-height: 22px;\r\n          float: left;\r\n          padding: 0 10px;\r\n\r\n          .summoney {\r\n            color: #c81623;\r\n            font-size: 16px;\r\n          }\r\n        }\r\n\r\n        .sumbtn {\r\n          float: right;\r\n\r\n          a {\r\n            display: block;\r\n            position: relative;\r\n            width: 96px;\r\n            height: 52px;\r\n            line-height: 52px;\r\n            color: #fff;\r\n            text-align: center;\r\n            font-size: 18px;\r\n            font-family: \"Microsoft YaHei\";\r\n            background: #e1251b;\r\n            overflow: hidden;\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n</style>"],"mappings":"AAqEA,SAAAA,UAAA;AACA;EACAC,IAAA;EACAC,OAAA;IACAC,QAAA;MACA;MACA,KAAAC,MAAA,CAAAC,QAAA;IACA;IACA;IACA,MAAAC,QAAAC,IAAA,EAAAC,MAAA,EAAAC,IAAA;MACA,QAAAF,IAAA;QACA;UACAC,MAAA;UACA;QACA;UACAA,MAAA,GAAAC,IAAA,CAAAC,MAAA;UACA;QACA;UACA,IAAAC,KAAA,CAAAH,MAAA,KAAAA,MAAA,MACA;YACAA,MAAA;UACA;YACAA,MAAA,GAAAI,QAAA,CAAAJ,MAAA,IAAAC,IAAA,CAAAC,MAAA;UACA;UACA;MACA;MACA;QACA,WAAAN,MAAA,CAAAC,QAAA;UAAAQ,KAAA,EAAAJ,IAAA,CAAAI,KAAA;UAAAH,MAAA,EAAAF;QAAA;QACA,KAAAL,OAAA;MACA,SAAAW,KAAA,GAEA;IACA;IACA;IACAC,eAAAN,IAAA,GAEA;EACA;EACAO,QAAA;IACA,KAAAb,OAAA;EACA;EACAc,QAAA;IACA,GAAAjB,UAAA;IACAkB,YAAA;MACA,YAAAC,OAAA,CAAAC,YAAA;IACA;IACA;IACAC,WAAA;MACA,IAAAC,GAAA;MACA,KAAAJ,WAAA,CAAAK,OAAA,CAAAC,IAAA;QACAF,GAAA,IAAAE,IAAA,CAAAd,MAAA,GAAAc,IAAA,CAAAC,QAAA;MACA;MACA,OAAAH,GAAA;IACA;IACA;IACAI,WAAA;MACA,YAAAR,WAAA,CAAAS,KAAA,CAAAH,IAAA,SAAAI,SAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}